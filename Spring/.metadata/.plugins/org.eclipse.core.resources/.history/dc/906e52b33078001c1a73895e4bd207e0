package com.institute.bean;

public class Employee {
       private int id;
       private String name;
       private Address add;
       private Experience expe;
        public Employee() {
        	
        }
		public Employee(int id, String name, Address add, Experience expe) {
			super();
			this.id = id;
			this.name = name;
			this.add = add;
			this.expe = expe;
		}
		public int getId() {
			return id;
		}
		public String getName() {
			return name;
		}
		public Address getAdd() {
			return add;
		}
		public Experience getExpe() {
			return expe;
		}
        
       
}
